namespace NumberTron {
  class Runner {
    static void Main(string[] args) {

    }
  }

  class Player {
    string name;
    string number;
    int tailSize;

    Player (string name, int number) {
        this.name = name;
        this.number = number;
        this.tailSize = 0;
    }

    void increaseTail() {
      this.tailSize += 1;
    }
  }
}
