namespace NumberTron {
  class Runner {
    static void Main(string[] args) {

    }
  }

  class Player {
    string name;
    string number;
    int tailSize;

    Player (string name, int number) {
        this.name = name;
        this.number = number;
        this.tailSize = 0;
    }

    void increaseTail() {
      this.tailSize += 1;
    }
  }

  class Tile {

    int value;

    Tile (int value) {
      this.value = value
    }

    void getTileValue() {
      return this.value
    }
  }

  class Board {
    
    Tile[][] board;

    Board (int row, int col) {
      this.board = Tile[row][col]
    }

    void getTileValue(int row, int col) {
      return this.board[row][col]
    }
  }
}
